<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="$.mapper.$">
    <!-- 회원 탈퇴 시 즐겨찾기 삭제 -->
    <delete id="deleteUserFavorites">
        DELETE FROM fav_gs
        WHERE user_id = #{userId}
    </delete>
</mapper>
